{
    "name" : "the entity name",                  // String
    "value" : 100,                               // Primitive
    "addresses" : [                              // Array of objects
                   {
                   "city" : "Shanghai",
                   "zipcode" : "200000"
                   },
                   {
                   "city" : "Beijing",
                   "zipcode" : "100000"
                   }
                   ],
    
    "followers" : [200, 201, 202, 205],          // Array of primitives
    
    "parent" : {                                 // Nested object
        "name" : "the inner entity name",
        "value" : null,                          // null as the value of a primitive
        "addresses" : [
                       {
                       "city" : "Shanghai inner",
                       "zipcode" : "200000"
                       },
                       {
                       "city" : "Beijing inner",
                       "zipcode" : "100000"
                       }
                       ],
        "followers": [],                          // Empty array
        "parent" : null                           // null object
    },
    
    "is_new?"   : true,                           // Key ending with '?' and starts with 'is_', '?' should be stripped
    "outdated?": true                             // Key ending with '?', should be mapped to 'isOutdated'
    
}